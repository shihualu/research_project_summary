+		save(nested);
+		save(new NestedAnnotatedModel(nested));
+		save(new NestedAnnotatedModel(nested));
+
+		assertEquals(1L, count(SimpleAnnotatedModel.class));
+		assertEquals(1L, count(RelationshipAnnotatedModel.class));
+		assertEquals(2L, count(NestedAnnotatedModel.class));
