diff --git a/aws-java-sdk-s3/src/main/java/com/amazonaws/mobileconnectors/s3/transferutility/Service.java b/aws-java-sdk-s3/src/main/java/com/amazonaws/mobileconnectors/s3/transferutility/Service.java
