-                    LOGGER.error("bulkInsert error : ", e);
-                } finally {
-                    database.endTransaction();
-                }
-                break;
-            default:
-                throw new IllegalArgumentException("Unknown URI: " + uri);
-        }
-        return mainUploadId;
-    }
