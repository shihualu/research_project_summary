+                + ((getIdentityPoolId() == null) ? 0 : getIdentityPoolId().hashCode());
+        hashCode = prime * hashCode + ((getRoles() == null) ? 0 : getRoles().hashCode());
+        hashCode = prime * hashCode
+                + ((getRoleMappings() == null) ? 0 : getRoleMappings().hashCode());
+        return hashCode;
+    }
