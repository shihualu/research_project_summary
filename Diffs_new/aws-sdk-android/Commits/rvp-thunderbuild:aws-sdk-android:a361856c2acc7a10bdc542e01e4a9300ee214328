+        enumMap.put("STOP_REQUESTED", STOP_REQUESTED);
+        enumMap.put("STOPPED", STOPPED);
         enumMap.put("IN_ERROR", IN_ERROR);
         enumMap.put("TRAINED", TRAINED);
     }
