+    public void setRules(java.util.Collection<MappingRule> rules) {
+        if (rules == null) {
+            this.rules = null;
+            return;
+        }
+
+        this.rules = new java.util.ArrayList<MappingRule>(rules);
+    }
+
+    /**
+     * <p>
+     * An array of rules. You can specify up to 25 rules per identity provider.
+     * </p>
