+    public DescribeIdentityResult withLogins(String... logins) {
+        if (getLogins() == null) {
+            this.logins = new java.util.ArrayList<String>(logins.length);
+        }
+        for (String value : logins) {
+            this.logins.add(value);
+        }
+        return this;
+    }
