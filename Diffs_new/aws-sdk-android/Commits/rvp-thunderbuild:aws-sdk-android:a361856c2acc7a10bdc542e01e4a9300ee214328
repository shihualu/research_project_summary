+    @Override
+    public boolean equals(Object obj) {
+        if (this == obj)
+            return true;
+        if (obj == null)
+            return false;
+
+        if (obj instanceof GetIdentityPoolRolesResult == false)
+            return false;
+        GetIdentityPoolRolesResult other = (GetIdentityPoolRolesResult) obj;
+
