+
+/**
+ * JSON unmarshaller for POJO ResultRow
+ */
+class ResultRowJsonUnmarshaller implements Unmarshaller<ResultRow, JsonUnmarshallerContext> {
+
+    public ResultRow unmarshall(JsonUnmarshallerContext context) throws Exception {
+        AwsJsonReader reader = context.getReader();
+        if (!reader.isContainer()) {
+            reader.skipValue();
+            return null;
+        }
+        ResultRow resultRow = new ResultRow();
+        reader.beginObject();
+        while (reader.hasNext()) {
+            String name = reader.nextName();
+            if (name.equals("GroupedBys")) {
+                resultRow.setGroupedBys(new ListUnmarshaller<ResultRowValue>(
+                        ResultRowValueJsonUnmarshaller.getInstance()
+                        )
+                                .unmarshall(context));
+            } else if (name.equals("Values")) {
+                resultRow.setValues(new ListUnmarshaller<ResultRowValue>(
+                        ResultRowValueJsonUnmarshaller.getInstance()
+                        )
+                                .unmarshall(context));
+            } else {
+                reader.skipValue();
+            }
+        }
+        reader.endObject();
+        return resultRow;
+    }
+
+    private static ResultRowJsonUnmarshaller instance;
+
+    public static ResultRowJsonUnmarshaller getInstance() {
+        if (instance == null)
+            instance = new ResultRowJsonUnmarshaller();
+        return instance;
+    }
+}
