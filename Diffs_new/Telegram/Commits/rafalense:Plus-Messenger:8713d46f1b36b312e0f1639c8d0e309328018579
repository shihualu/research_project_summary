-                if (mPauseTime > 0) {
-                    mDelayStartTime += (currentTime - mPauseTime);
-                }
-            }
-            long deltaTime = currentTime - mDelayStartTime;
-            if (deltaTime > mStartDelay) {
-                mStartTime = currentTime - (deltaTime - mStartDelay);
-                mPlayingState = RUNNING;
-                return true;
-            }
-        }
-        return false;
-    }
