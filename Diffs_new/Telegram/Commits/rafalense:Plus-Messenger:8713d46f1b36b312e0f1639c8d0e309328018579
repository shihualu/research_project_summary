+            req_msg_id = stream.readInt64(exception);
+            now = stream.readInt32(exception);
+            int count = stream.readInt32(exception);
+            for (int a = 0; a < count; a++) {
+                TL_futureSalt salt = new TL_futureSalt();
+                salt.readParams(stream, exception);
+                salts.add(salt);
+            }
         }
 
