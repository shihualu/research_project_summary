+    private void redraw(final int count) {
+        if (count < 6) {
+            if (containerView != null) {
+                containerView.invalidate();
+                AndroidUtilities.runOnUIThread(new Runnable() {
+                    @Override
+                    public void run() {
+                        redraw(count + 1);
+                    }
+                }, 100);
+            }
+        }
+    }
+
+    public void onResume() {
+        redraw(0); //workaround for camera bug
+    }
