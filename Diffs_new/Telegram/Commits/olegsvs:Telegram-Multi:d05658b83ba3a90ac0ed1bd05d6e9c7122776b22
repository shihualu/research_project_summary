+
+
+        itemList = new ArrayList<Object>();
+        int usersCount = getUsersCount();
+        for (int i = 0; i < usersCount ; i++) {
+            AddObjectToList(R.drawable.book_user, "User" , getUserTag("_user_" + i));
+        }
+    }
