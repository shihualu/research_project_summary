-    private void handleAnnounce() {
-        Set<String> permissions = AccessToken.getCurrentAccessToken().getPermissions();
-        if (!permissions.contains(PERMISSION)) {
-            pendingAnnounce = true;
-            requestPublishPermissions();
-            return;
