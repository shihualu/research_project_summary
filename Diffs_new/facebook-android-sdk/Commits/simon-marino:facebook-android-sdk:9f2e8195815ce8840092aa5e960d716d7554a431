-
-        if (cursor != null) {
-            deliverResult(cursor);
-        }
-    }
-
-    private void requestCompleted(GraphResponse response) {
-        GraphRequest request = response.getRequest();
-        if (request != currentRequest) {
-            return;
-        }
-
-        loading = false;
-        currentRequest = null;
-
